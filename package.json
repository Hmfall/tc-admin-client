    "tslib": "=2.6.2",
    "typescript": "=5.3.3",
    "unplugin-auto-import": "=0.18.3",
    "vite": "=6.0.11",
    "vite-plugin-vuetify": "=2.0.4",
    "vue-eslint-parser": "=9.4.0",
    "vue-tsc": "=1.8.27"
